public Integer rockPaperScissors(String player1, String player2) {
    //code here
    Map<String,String> winningRule = new Map<String, String>();
    winningRule.put('rock', 'scissors');
    winningRule.put('paper', 'rock');
    winningRule.put('scissors', 'paper');


    if(winningRule.get(player1) == player2){
        return 1;
    }else if(winningRule.get(player2) == player 1){
        return 2;
    }else{
        return 0;
    }

}