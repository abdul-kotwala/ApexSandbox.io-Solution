public static Integer findLargest(Integer num1, Integer num2, Integer num3) {
	//code here
    List<Integer> listOfIntegers = new List<Integer>();
    listOfIntegers.add(num1);
    listOfIntegers.add(num2);
    listOfIntegers.add(num3);
    listOfIntegers.sort();
    return listOfIntegers.get(listOfIntegers.size() - 1);
}