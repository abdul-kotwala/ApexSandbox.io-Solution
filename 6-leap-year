public Boolean isLeapYear(Integer year) {
    //code here
    Integer remainderBy4 = Math.mod(year,4);
    Integer remainderBy100 = Math.mod(year,100);
    Integer remainderBy400 = Math.mod(year,400);
    if(remainderBy4 == 0){
        if(remainderBy100 != 0){
            return true;
        }
    }else{
        return false;
    }

}
